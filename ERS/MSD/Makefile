#------------------------------------------------------------------------------
#
#       MSD makefile
#
#------------------------------------------------------------------------------

#------------------------------------------------------------------------------
# Platform dependent stuff:
#------------------------------------------------------------------------------
 F77 = g77
 F77FLAGS = -O3 
 LIBS =
 DFLAGS =
 LDFLAGS = 
 LD = g77
#------------------------------------------------------------------------------

.f.o:
	$(F77) $(F77FLAGS) -c $*.f

 EXECS = MSD

 all : $(EXECS)

 OBJECTS = MSD.o Parser.o GetMass.o LowCase.o

 MSD : $(OBJECTS)
	$(LD) $(DFLAGS) -o MSD $(OBJECTS) $(LDFLAGS)

 clean :
	rm -f *.o
	rm -f $(EXECS)

